Function Checkpoint_Text(&HeaderRecord As Record, &HeaderButtonRecord As Record)
   If &HeaderRecord.EP_REVIEW_STATUS.Value = "TP" Then
      Evaluate &EP_GBLKEYS_WRK.EP_USER_ROLE.Value
      When = "M"
         If (&HeaderRecord.EP_TP_UPD_BY_FLG.Value = "M" Or
               &HeaderRecord.EP_TP_UPD_BY_FLG.Value = "B") And
               &HeaderRecord.EP_MGR_CMNT_SHARED.Value = "Y" And
               &HeaderRecord.EP_REVIEW_STATUS.Value <> "CA" Then
            &HeaderButtonRecord.EP_HTMLAREA1.Value = GetText("APPR_BASE1_INST3", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRecord.EP_REVIEW_STATUS.Value, " ", " ", " ", " ", " ");
         End-If;
         Break;
      When = "E"
         If &HeaderRecord.EP_TP_UPD_BY_FLG.Value = "B" And
               &HeaderRecord.EP_TP_EE_CMNTS_SW.Value = "N" And
               &HeaderRecord.EP_MGR_CMNT_SHARED.Value = "Y" Then
            &HeaderButtonRecord.EP_HTMLAREA1.Value = GetText("APPR_BASE1_INST3", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRecord.EP_REVIEW_STATUS.Value, " ", " ", " ", " ", " ");
         End-If;
         
         If &HeaderRecord.EP_TP_UPD_BY_FLG.Value = "B" And
               &HeaderRecord.EP_TP_EE_CMNTS_SW.Value = "Y" And
               &HeaderRecord.EP_EE_CMNT_SHARED.Value = "Y" Then
            &HeaderButtonRecord.EP_HTMLAREA1.Value = GetText("APPR_BASE1_INST3", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRecord.EP_REVIEW_STATUS.Value, " ", " ", " ", " ", " ");
         End-If;
         
         If &HeaderRecord.EP_TP_UPD_BY_FLG.Value = "M" And
               &HeaderRecord.EP_TP_EE_CMNTS_SW.Value = "N" And
               &HeaderRecord.EP_MGR_CMNT_SHARED.Value = "N" Then
            &HeaderButtonRecord.EP_HTMLAREA1.Value = GetText("APPR_BASE1_INST3", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRecord.EP_REVIEW_STATUS.Value, " ", " ", " ", " ", " ");
         End-If;
         
         Break;
         
      When-Other
         Break;
      End-Evaluate;
   End-If;
End-Function;


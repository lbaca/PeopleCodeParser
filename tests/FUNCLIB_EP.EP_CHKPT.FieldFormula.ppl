Declare Function GetText5keys PeopleCode FUNCLIB_EP.HR_SSTEXT_KEY1 FieldFormula;

Global Record &EP_GBLKEYS_WRK;
Global boolean &HasAccess_EC, &HasAccess_TP, &HasAccess_EV, &NoAccess, &ShowStartPage, &GBL_Step_Cmplt, &Gbl_same_focus, &GBL_TP_same_focus;
Component Row &rowChkpt1; /*Check Point 1*/
Component Row &rowChkpt2; /* Check Point 2*/
Component Row &rowChkpt3; /* Check Point 3*/
Component Row &rowChkpt4; /* Check Point 4*/
Component Row &rowChkpt5; /* Check Point 5*/
Component Row &rowChkpt6; /* Check Point 6*/
Component Row &rowChkpt7; /* Check Point 7*/
Component Row &rowChkpt8; /* Check Point 8*/
Component Row &rowChkpt9; /* Check Point 9*/
Component Row &rowChkpt10; /* Check Point 10*/
Component Row &rowChkpt11; /* Check Point 11*/
Component Row &rowFinal; /* Final Goals */
Local string &Skip_flg;

Function Checkpoints_Behavior(&rsStep As Rowset, &HeaderRec As Record)
   
   
   Evaluate &HeaderRec.EP_TRKPROG_STATUS.Value
      /* Not Applicable, hide all Checkpoint steps */
   When = "NA" /* Not Applicable */
      &rowChkpt1.Visible = False;
      &rowChkpt2.Visible = False;
      &rowChkpt3.Visible = False;
      &rowChkpt4.Visible = False;
      &rowChkpt5.Visible = False;
      &rowChkpt6.Visible = False;
      &rowChkpt7.Visible = False;
      &rowChkpt8.Visible = False;
      &rowChkpt9.Visible = False;
      &rowChkpt10.Visible = False;
      &rowChkpt11.Visible = False;
      Break;
      
      
   When = "NS" /* Not Started */
   When = "IP" /* In Progress */
   When = "CO" /* Complete */
      /* Only Manager Or Employee Role should see the Checkpoint Steps, other role: Approver, HR Admin, Other and View-Only should not see the Track Progress Step */
      
      If &EP_GBLKEYS_WRK.EP_USER_ROLE.Value = "O" Then
         &rowChkpt1.Visible = False;
         &rowChkpt2.Visible = False;
         &rowChkpt3.Visible = False;
         &rowChkpt4.Visible = False;
         &rowChkpt5.Visible = False;
         &rowChkpt6.Visible = False;
         &rowChkpt7.Visible = False;
         &rowChkpt8.Visible = False;
         &rowChkpt9.Visible = False;
         &rowChkpt10.Visible = False;
         &rowChkpt11.Visible = False;
      End-If;
      
      
      /* Add Check Point Steps label */
      &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "1", " ", " ", " ", " ", " ");
      &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "2", " ", " ", " ", " ", " ");
      &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "3", " ", " ", " ", " ", " ");
      &rowChkpt4.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "4", " ", " ", " ", " ", " ");
      &rowChkpt5.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "5", " ", " ", " ", " ", " ");
      &rowChkpt6.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "6", " ", " ", " ", " ", " ");
      &rowChkpt7.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "7", " ", " ", " ", " ", " ");
      &rowChkpt8.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "8", " ", " ", " ", " ", " ");
      &rowChkpt9.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "9", " ", " ", " ", " ", " ");
      &rowChkpt10.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "10", " ", " ", " ", " ", " ");
      &rowChkpt11.EP_BTN_LINK_WRK.EP_STEP_LINK.Label = GetText5keys("TP_DOC_SUB_STATUS", " ", " ", " ", &EP_GBLKEYS_WRK.EP_USER_ROLE.Value, &HeaderRec.EP_REVIEW_STATUS.Value, "11", " ", " ", " ", " ", " ");
      
   When = "NS" /* Not Started */
   When = "IP" /* In Progress */
      
      /* show  all Check point step "not started" icons first */
      
      &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt4.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt5.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt6.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt7.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt8.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt9.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt10.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      &rowChkpt11.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
      
      
      /* Check point 1 Step */
      
      Evaluate &HeaderRec.EP_TP_NBR_CHECKPTS.Value
      When = 1
         /* If Checkpoint 1 is first step, none Establish Criteria Step */
         /* Set the Step Status Image button to display 'in progress'. */
         /* Expand the Check point 1 Step */
         If &HeaderRec.EP_EST_STATUS.Value = "NA" Or
               &HeaderRec.EP_EST_STATUS.Value = "CO" Then
            If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
               /* When Estalish Criteria is approved, Check Point 1 icon should be not started */
               &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
               If &GBL_Step_Cmplt Then
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               Else
                  
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               End-If;
               
               &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               
               
               /* If it is from No Access Page or Confirmation PAge, then collapse the sub-steps */
               If &GBL_Step_Cmplt Or
                     &NoAccess Then
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                  &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
                  
                  
               End-If;
            Else
               &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PS_STATUS_CANCELLED_ICN;
               &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON4", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
               &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               
               
               
            End-If;
         End-If;
         &rowFinal.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
         &rowFinal.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
         &rowFinal.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
         &rowChkpt2.Visible = False;
         &rowChkpt3.Visible = False;
         &rowChkpt4.Visible = False;
         &rowChkpt5.Visible = False;
         &rowChkpt6.Visible = False;
         &rowChkpt7.Visible = False;
         &rowChkpt8.Visible = False;
         &rowChkpt9.Visible = False;
         &rowChkpt10.Visible = False;
         &rowChkpt11.Visible = False;
         Break;
         
         /* Check Point 2 Step */
         /* When It's Checkpoint Nbr equal to 2*/
      When = 2
         Evaluate &HeaderRec.EP_TP_SUB_STATUS.Value
         When = "1"
            If &HeaderRec.EP_EST_STATUS.Value = "NA" Or
                  &HeaderRec.EP_EST_STATUS.Value = "CO" Then
               If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
                  
                  If &GBL_Step_Cmplt Then
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                     
                  Else
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                     
                  End-If;
                  
                  /* Expand the Check point 1 Step */
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
                  &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
                  If &GBL_Step_Cmplt Or
                        &NoAccess Then
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                     &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
                  End-If;
               Else
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PS_STATUS_CANCELLED_ICN;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON4", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
                  &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
                  
               End-If;
            End-If;
            Break;
            
         When = "2"
            
            If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
               /* Set the Step Status Image button to display 'in progress'. */
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
               If &GBL_Step_Cmplt Then
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               Else
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               End-If;
               /* Expand the Check point 2 Step */
               &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               If &GBL_Step_Cmplt Or
                     &NoAccess Then
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                  &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
               End-If;
            Else
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PS_STATUS_CANCELLED_ICN;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON4", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
               &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               
            End-If;
            
            /* Collapse Check Point 1 step, set step status Icon to 'Completed' */
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_COMPLETED;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON3", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
            
            &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
            &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
            
            Break;
            
            
            
         End-Evaluate;
         &rowFinal.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
         &rowFinal.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
         &rowFinal.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
         /* Hide Check point 3-11 Steps */
         &rowChkpt3.Visible = False;
         &rowChkpt4.Visible = False;
         &rowChkpt5.Visible = False;
         &rowChkpt6.Visible = False;
         &rowChkpt7.Visible = False;
         &rowChkpt8.Visible = False;
         &rowChkpt9.Visible = False;
         &rowChkpt10.Visible = False;
         &rowChkpt11.Visible = False;
         Break;
         
         /* Check point 3 Step */
      When = 3
         Evaluate &HeaderRec.EP_TP_SUB_STATUS.Value
         When = "1"
            If &HeaderRec.EP_EST_STATUS.Value = "NA" Or
                  &HeaderRec.EP_EST_STATUS.Value = "CO" Then
               If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
                  If &GBL_Step_Cmplt Then
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                     
                  Else
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                     &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                     
                  End-If;
                  /* Expand the Check point 1 Step */
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
                  &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
                  /*If from Comfirmation or No Access Page, collapse the Check Point 1 */
                  If &GBL_Step_Cmplt Or
                        &NoAccess Then
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                     &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                     &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
                  End-If;
                  &rowFinal.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                  &rowFinal.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                  &rowFinal.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
               Else
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PS_STATUS_CANCELLED_ICN;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON4", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
                  &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
                  &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
                  
               End-If;
               
               
            End-If;
            Break;
            
         When = "2"
            /* Set the Step Status Image button to display 'in progress'. */
            If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
               If &GBL_Step_Cmplt Then
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               Else
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               End-If;
               /* Expand the Check point 2 Step */
               &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               If &GBL_Step_Cmplt Or
                     &NoAccess Then
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
                  &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
                  &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
               End-If;
               
            Else
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PS_STATUS_CANCELLED_ICN;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON4", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
               &rowChkpt2.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt2.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt2.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               
            End-If;
            
            
            /* Collapse Check Point 1 step, set step status Icon to 'Completed' */
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_COMPLETED;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON3", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
            
            &rowChkpt1.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = True;
            &rowChkpt1.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = False;
            &rowChkpt1.DERIVED_EP.EP_GROUPBOX_WC1.Visible = False;
            
            Break;
         When = "3"
            /* Set the Step Status Image button to display 'in progress'. */
            If &HeaderRec.EP_REVIEW_STATUS.Value <> "CA" Then
               &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Visible = True;
               If &GBL_Step_Cmplt Then
                  &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_ASSIGNED;
                  &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON1", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               Else
                  &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.LabelImage = Image.PTAI_IN_PROGRESS;
                  &rowChkpt3.EP_BTN_LINK_WRK.EP_STEP_STATUS_IMG.Label = GetText("APPR_WC_ICON2", " ", " ", " ", " ", " ", " ", " ", " ", " ", " ");
                  
               End-If;
               /* Expand the Check point 3 Step */
               &rowChkpt3.EP_BTN_LINK_WRK.EP_EXP_STEP_PB.Visible = False;
               &rowChkpt3.EP_BTN_LINK_WRK.EP_CLP_STEP_PB.Visible = True;
               &rowChkpt3.DERIVED_EP.EP_GROUPBOX_WC1.Visible = True;
               If &GBL_Step_Cmplt Or
                     &NoAccess Then
